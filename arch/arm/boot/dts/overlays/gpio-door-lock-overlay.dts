/dts-v1/;
/plugin/;

/ {
	compatible = "brcm,bcm2835";

	fragment@0 {
		target-path = "/";
		__overlay__ {
			lock0: gpio-door-lock@0 {
				compatible = "door_lock";
				lock-gpios = <&gpio 37 0>;
				interrupt-parent = <&gpio>;
				interrupts = <27 2>; // high-to-low edge triggered
				open-time = <3000>; //milliseconds
			};
		};
	};
};
